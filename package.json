{
  "name": "vite-vanilla-ts-template",
  "description": "Vite starter template for vanilla Typescript with some basic dependencies.",
  "version": "0.1.1",
  "keywords": [
    "vanilla",
    "javascript",
    "template",
    "boilerplate",
    "boilerplate-template",
    "postcss",
    "eslint",
    "prettier",
    "scaffold",
    "scaffold-template",
    "vite",
    "vitejs",
    "vite-template",
    "vitejs-template"
  ],
  "scripts": {
    "dev": "vite --host --open",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"./**/*.{ts,tsx,json,css}\"",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "tsc && vite build",
    "preview": "vite preview --open",
    "buildpreview": "tsc && vite build && vite preview --open",
    "release": "npm test && npm run build && changelogen --release --push && npm publish",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  },
  "dependencies": {
    "the-new-css-reset": "^1.11.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.2.0",
    "@nabla/vite-plugin-eslint": "^2.0.4",
    "@types/node": "^20.12.8",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "camelcase": "^8.0.0",
    "changelogen": "^0.5.5",
    "cssnano": "^7.0.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-pretty": "^6.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-nesting": "^12.1.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.6.0"
  }
}
